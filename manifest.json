{
 "manifest_version": 2,
 "name": "Reshape",
 "description": "Dynamic web content modification for special needs.",
 "version": "0.1",
 "author": "Shubham Jindal",
"browser_action": {
   "default_title": "Have a good day"
 },
 "background": {
       "scripts": [
         "background.js"
       ],
       "persistent": false
     },
"options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
"chrome_url_overrides" : {
  "newtab": "Test.html"
},
 "permissions": ["activeTab"]
}